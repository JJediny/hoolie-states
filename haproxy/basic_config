global
    log 127.0.0.1 local0 notice
    maxconn 200
    user haproxy
    group haproxy


defaults
    log      global
    mode     http
    option   httplog
    option   dontlognull
    retries  3
    option   redispatch
    timeout connect   5000
    timeout client    10000
    timeout server    10000

frontend demo-http
    bind *:80
    default_backend demo-servers

backend demo-servers
    server web-server1 10.5.1.126 check
    server web-server2 10.5.1.234 check
    server ubuntu1 10.5.1.43 check
